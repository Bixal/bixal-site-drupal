<div class="person-profile" ismobile="false">
  <div class="person-profile__main">
    <div class="person-profile__main-container">
      <div class="person-profile__intro">
        <div class="person-profile__image">
          <div class="person-profile__image-placeholder">
            <div class="person-profile__image-positioner">
              <div
                data-gatsby-image-wrapper=""
                class="gatsby-image-wrapper"
                style="width: 100%; height: 100%"
              >
                <div
                  aria-hidden="true"
                  data-placeholder-image=""
                  style="
                    opacity: 0;
                    transition: opacity 500ms linear;
                    object-fit: cover;
                  "
                ></div>
                <picture>
                  <source
                    type="image/webp"
                    srcset="https://bixal.com/static/ac0c71983b8cf0d4955a0729c7a8ba71/ad0d6/amie-nisler.webp 600w"
                    sizes="600px" />
                  <img
                    width="600"
                    height="600"
                    data-main-image=""
                    style="object-fit: cover; opacity: 1"
                    sizes="600px"
                    decoding="async"
                    loading="lazy"
                    src="https://bixal.com/static/ac0c71983b8cf0d4955a0729c7a8ba71/d0c36/amie-nisler.jpg"
                    srcset="https://bixal.com/static/ac0c71983b8cf0d4955a0729c7a8ba71/d0c36/amie-nisler.jpg 600w
                    "
                    alt="Amie Nisler"
                    />
                </picture>
                <noscript>
                  <picture>
                    <source
                      type="image/webp"
                      srcSet="https://bixal.com/static/ac0c71983b8cf0d4955a0729c7a8ba71/ad0d6/amie-nisler.webp 600w"
                      sizes="600px" />
                    <img
                      width="600"
                      height="600"
                      data-main-image=""
                      style="object-fit: cover; opacity: 1"
                      sizes="600px"
                      decoding="async"
                      loading="lazy"
                      src="https://bixal.com/static/ac0c71983b8cf0d4955a0729c7a8ba71/d0c36/amie-nisler.jpg"
                      srcSet="https://bixal.com/static/ac0c71983b8cf0d4955a0729c7a8ba71/d0c36/amie-nisler.jpg 600w"
                      alt="Amie Nisler" />
                      </picture
                ></noscript>
              </div>
            </div>
          </div>
        </div>
        <div class="person-profile__description">
          <h1 class="person-profile__title">Amie Nisler</h1>
          <div class="person-profile__job-description">
            <div class="person-profile__role">
              Senior Communications Strategist
            </div>
          </div>
          <hr class="person-profile__decorative-divider" />
          <div class="social-nav">
            <ul class="social-nav__list">
              <li class="social-nav__item">
                <a
                  href="https://www.linkedin.com/in/amienisler/"
                  class="social-nav__link"
                  target="_blank"
                  rel="noopener noreferrer"
                  ><span class="visually-hidden">linkedIn</span>
                  <div class="icon-container">
                    <img
                      src="https://bixal.com/static/45d1bb1b8ac9ab4b39726b1376923f54/linkedIn.svg"
                      alt="linkedIn logo"
                    />
                    </div>
                  </a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="person-profile__aside">
    <div class="person-profile__aside-container">
      <div class="person-profile__interests">
        <div class="person-profile__specialties">
          <h2>Specialties</h2>
          <ul>
            <li>Behavioral Science</li>
            <li>Data Systems</li>
            <li>Emergency and Risk Communications</li>
            <li>Health Communications</li>
            <li>Plain Language</li>
          </ul>
        </div>
        <div class="person-profile__bucket-list">
          <h2>Bucket List</h2>
          <p class="profile-bucketlist">To attend a World Cup final game.</p>
        </div>
      </div>
    </div>
  </div>
</div>
