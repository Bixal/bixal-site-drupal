{#
 /**
  * @file
  * Theme override to display a paragraph - View embed.
  *
  * Drupal variables:
  * - field_view_name: (taxonomy reference: views) Required - Term for Views Vocab
  *
  */
#}
{% extends  "paragraph--default.html.twig" %}

{% set display_name = content.field_view_name.0['#entity'].field_display_name.0.value %}
{% set machine_name = content.field_view_name.0['#entity'].field_machine_name.0.value %}

{% block content %}
  {{ content.contextual_links }}
  {{ drupal_view(machine_name, display_name) }}
{% endblock %}
